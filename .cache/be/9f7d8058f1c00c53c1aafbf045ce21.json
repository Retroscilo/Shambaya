n    transform: translate(-50%) rotate(360deg);\n  }\n}\n\n/** ROOM **/\n\n.container--room {\n  margin: 200px auto;\n  width: 80vw;\n  height: 40vh;\n  background: wheat;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-evenly;\n  align-items: center;\n}\n\n.room {\n  width: 22%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n}\n\n.r1 {\n  background: rosybrown;\n}\n\n.r2 {\n  background: royalblue;\n}\n\n.r3 {\n  background: saddlebrown;\n}\n\n.r4 {\n  background: salmon;\n}\n\n.circle img {\n  position: relative;\n  width: 400px;\n  top: 50%;\n  left: 50%;\n  transform: translate(-47%, -48%);\n}\n\n.title p {\n  width: 1200px;\n  font-size: 1.5em;\n  font-weight: 500;\n  line-height: 1.5em;\n  letter-spacing: 0.1em;\n  text-align: center;\n  margin: 150px auto;\n}\n\n.circle {\n  position: relative;\n  left: 50%;\n  width: 400px;\n  height: 400px;\n  transform: translate(-50%, -10%);\n  border-radius: 50%;\n  background: white;\n\n}\n\n/** Caroussel **/\n.caroussel {\n  position: relative;\n  background: white;\n}\n\n.c {\n  position: absolute;\n  width: 50vw;\n  height: 60vh;\n  background-repeat: no-repeat;\n  background-size: cover;\n  background-position: center;\n  transition: all 0.5s;\n}\n\n.c1 {\n  background-image: url(\"./assets/SalonBanuwa.jpg\");\n}\n\n.c2 {\n  opacity: 0;\n  background-image: url(\"./assets/SalonSylvana.jpg\");\n}\n\n.c3 {\n  opacity: 0;\n  background-image: url(\"./assets/singing-bowl-233991_960_720.jpg\");\n}\n\n.c4 {\n  opacity: 0;\n  background-image: url(\"./assets/Accueil.jpg\");\n}\n\n.filler {\n  height: 60vh;\n  width: 50vw;\n}\n\n.wrapper {\n  width: 50vw;\n  height: 10vh;\n  position: relative;\n  display: flex;\n}\n\n.w {\n  width: 20vw;\n  transition: all 0.5s;\n}\n\n.w1 {\n  background: grey;\n}\n\n.w2 {\n  background: gold;\n}\n\n.w3 {\n  background: greenyellow;\n}\n\n.w4 {\n  background: chocolate;\n}\n\n.active {\n  width: 40vw;\n}\n\n/** Therapist + Particulier **/\n.therapist {\n  margin-top: 100px;\n}\n\n.therapist,\n.particulier,\n.entreprise {\n  height: 50vh;\n  width: 80vw;\n  display: flex;\n  flex-direction: column;\n  margin-left: 100px;\n}\n\n.section h2 {\n  height: 5vh;\n  display: flex;\n  align-items: center;\n  font-size: 2.5em;\n  margin-bottom: 30px;\n}\n\n"},"lineCount":null}},"error":null,"hash":"82c28b1f5d905a7aa5778b4c73eeba96","cacheData":{"env":{}}}